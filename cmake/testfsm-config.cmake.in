# testFSM CMake config file
#
# This file sets the following variables:
# testFSM_FOUND - Always TRUE.
# testFSM_INCLUDE_DIRS - Directories containing the testFSM include files.
# testFSM_IDL_DIRS - Directories containing the testFSM IDL files.
# testFSM_LIBRARIES - Libraries needed to use testFSM.
# testFSM_DEFINITIONS - Compiler flags for testFSM.
# testFSM_VERSION - The version of testFSM found.
# testFSM_VERSION_MAJOR - The major version of testFSM found.
# testFSM_VERSION_MINOR - The minor version of testFSM found.
# testFSM_VERSION_REVISION - The revision version of testFSM found.
# testFSM_VERSION_CANDIDATE - The candidate version of testFSM found.

message(STATUS "Found testFSM-@PROJECT_VERSION@")
set(testFSM_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(testFSM_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(testFSM_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(testFSM_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(testFSM_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(testFSM_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(testFSM_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(testFSM_DEFINITIONS ${<dependency>_DEFINITIONS})

set(testFSM_VERSION @PROJECT_VERSION@)
set(testFSM_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(testFSM_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(testFSM_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(testFSM_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

